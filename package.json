{
  "name": "kontakt-gql",
  "version": "1.0.0",
  "description": "Yet another Contact List Manager Using Nestjs and GraphQL",
  "author": "Shady Khalifa <shekohex@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && webpack --config webpack.prod.js",
    "start:prod": "node build/main.js",
    "start:hmr": "node dist/server",
    "dev": "npm-run-all --parallel start:hmr playground",
    "playground": "graphql playground",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "engines": {
    "node": ">= 9.2",
    "npm": ">= 6.1.0"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/graphql": "^3.0.0",
    "apollo-server-express": "^2.0.2",
    "bcryptjs": "^2.4.3",
    "core-js": "^2.5.7",
    "dotenv": "^6.0.0",
    "fastify-formbody": "^2.0.0",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tools": "^3.1.1",
    "jsonwebtoken": "^8.3.0",
    "nanoid": "^1.2.0",
    "prisma-binding": "^2.1.4",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/bcryptjs": "^2.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/ramda": "^0.25.36",
    "@types/supertest": "^2.0.4",
    "awesome-typescript-loader": "^5.2.0",
    "graphql-cli": "^2.16.5",
    "graphql-import-loader": "^0.2.1",
    "hard-source-webpack-plugin": "^0.12.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.11.1",
    "prisma": "^1.14.0",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
